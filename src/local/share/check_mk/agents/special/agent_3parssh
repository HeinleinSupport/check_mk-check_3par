#!/bin/bash

if [ "$1" == "" ]
then
	echo Invalid usage : agent_3parssh InServ
	exit 3
fi

echo "<<<3parssh>>>"
echo "---statcpu---"
ssh $2@$1 statcpu -d 1 -iter 1 -t
echo "---statcpu---"

echo "---shownode---"
ssh $2@$1 shownode -verbose
echo "---shownode---"

echo "---showcage---"
ssh $2@$1 showcage -d
echo "---showcage---"
